//---------------------------------------------------------------------------

#pragma hdrstop

#include "UCSMemoria.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)

CSMemoria::CSMemoria(){
	mem = new Nodo[max]
	for(int i=0;i<max;i++){
		mem[i].dato=0;
		mem[i].link= i+1;
	}
	mem[max-1].link=-1;
	libre=0;
}