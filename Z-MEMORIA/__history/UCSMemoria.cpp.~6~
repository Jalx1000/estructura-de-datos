//---------------------------------------------------------------------------

#pragma hdrstop

#include "UCSMemoria.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)

CSMemoria::CSMemoria()
{
	mem = new Nodo[MAX];
	for (int i = 0; i < MAX; i++){
		mem[i].dato = 0;
		mem[i].link = i + 1;
	};
    mem[MAX- 1].link = -1;
    libre = 0;
}

void CSMemoria::mostrar_memoria(){
cout<<"hola\n";
}

